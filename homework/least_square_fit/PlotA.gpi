set terminal svg
set output "PlotA.svg"
set xlabel "t time [days]"
set ylabel "Activity (relative units)"
set title "Radioactivity ThX (224 Ra)"
set terminal svg background "white"

set key right top
set grid


plot "dataPartABC.data" with yerrorbars title "Measured data" \
	, "dataPartABC_fit.data" using 1:2 with lines title "Fit A=a*exp(-l*t)"\
	, "dataPartABC_fit.data" using 1:3:4 with filledcurves close title "Fit 2-sigma inteval"
