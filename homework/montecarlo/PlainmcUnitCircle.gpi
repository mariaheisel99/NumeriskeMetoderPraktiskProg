set terminal svg
set output "PlainmcUnitCircle.svg"
set xlabel "#Points"
set ylabel ""
set title "Unit Circle Plain MC"
set terminal svg background "white"
set key box top
set grid 
plot [0:100000] [0:0.06] "out.Circle.txt" using 1:3 with lines title "Estimated error" \
	,  "out.Circle.txt"  using 1:4 with linespoints title "Actual error " \
	, "out.Circle.txt"  using 1:5 with lines title "1/sqrt(n) fit to Actual error"
